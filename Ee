import mysql.connector

execute_query = lambda host, user, password, database, query: (
    lambda conn, cursor: (
        cursor.execute(query),
        [dict(zip(cursor.column_names, row)) for row in cursor.fetchall()]
    )
)(mysql.connector.connect(host=host, user=user, password=password, database=database), conn.cursor(dictionary=True))

# Example usage:
host = "your-rds-endpoint"
user = "your-username"
password = "your-password"
database = "your-database"
query = "SELECT * FROM your_table"
result = execute_query(host, user, password, database, query)
print(result)
